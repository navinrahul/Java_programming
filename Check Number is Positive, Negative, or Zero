import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
import java.util.Scanner;

public class Check        {
        public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        sc.close();
        if (num > 0) {
          System.out.println("positive");
        } else if (num < 0) {
          System.out.println("negative");
        } else {
           System.out.println("zero");
        }
        }
        }
